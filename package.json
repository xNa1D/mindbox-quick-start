    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^2.4.5",
    "prettier": "^2.5.1",
    "style-loader": "^2.0.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.5.3",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  }
}
